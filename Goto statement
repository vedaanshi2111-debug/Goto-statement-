#include <iostream>
using namespace std;

int main() {
    int numbers[] = {1, 3, 5, 7, 8, 9};
    int size = 6;

    for (int i = 0; i < size; i++) {
        if (numbers[i] % 2 == 0) {
            goto found;  // Jump to the label 'found'
        }
    }

    cout << "No even number found." << endl;
    return 0;

found:  // Label
    cout << "First even number is: " << numbers[i] << endl;

    return 0;
}
